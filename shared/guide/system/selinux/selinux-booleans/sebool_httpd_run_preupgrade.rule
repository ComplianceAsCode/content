references:
    disa: TBD
    nist: TBD
    srg: TBD
    stigid@rhel7: TBD

ocil_clause: 'httpd_run_preupgrade is not disabled'

ocil: '<sebool-check-macro enable="false" sebool="httpd_run_preupgrade" />'

oval_variables:
    - var_httpd_run_preupgrade
