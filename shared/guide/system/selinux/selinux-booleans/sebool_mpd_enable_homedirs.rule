    srg: TBD
    stigid@rhel7: TBD

ocil_clause: 'mpd_enable_homedirs is not disabled'

ocil: '<sebool-check-macro enable="false" sebool="mpd_enable_homedirs" />'

oval_variables:
    - var_mpd_enable_homedirs
