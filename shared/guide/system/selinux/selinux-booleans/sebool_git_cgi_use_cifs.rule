    cce@rhel7: RHEL7-CCE-TBD

references:
    disa: TBD
    nist: TBD
    srg: TBD
    stigid@rhel7: TBD

ocil_clause: 'git_cgi_use_cifs is not disabled'

ocil: '<sebool-check-macro enable="false" sebool="git_cgi_use_cifs" />'

oval_variables:
    - var_git_cgi_use_cifs
