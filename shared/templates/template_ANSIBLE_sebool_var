# platform = multi_platform_rhel, multi_platform_fedora
# reboot = false
# strategy = enable
# complexity = low
# disruption = low
- name: Set SELinux boolean %SEBOOLID% accordingly
  seboolean:
    name: %SEBOOLID%
    state: (ansible-populate var_%SEBOOLID%)
    persistent: yes
  tags:
    @ANSIBLE_TAGS@

