      <title>Is max_request_body_size parameter set to 114688?</title>
      <affected family="unix">
        <platform>multi_platform_all</platform>
      </affected>
      <description>Is max_request_body_size parameter set to 114688?</description>
    </metadata>
    <criteria comment="Is max_request_body_size parameter set to 114688?" operator="AND">
